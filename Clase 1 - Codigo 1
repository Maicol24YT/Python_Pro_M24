import random
#Tengo 10 a単os este es mi codigo y se trata de crear una contrase単a aleatoria solo dime y lo hago automaticamente

characters_v = "+-/*!&$#?=@abcdefghijklnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890"


character_select = int(input("Dime la longitud de la contrase単a:   "))

contrasena = ""

for i in range(character_select):
    contrasena += random.choice(characters_v)

print(f'Tu contrase単a es {contrasena}')
